{
  "publisher": "colder",
  "name": "source-map-visualization",
  "displayName": "Source Map Visualization",
  "version": "2.1.2",
  "private": true,
  "packageManager": "pnpm@7.18.2",
  "description": "A simple visualization of source map data",
  "author": "Colder",
  "license": "MIT",
  "homepage": "https://github.com/cncolder/vscode-source-map-visualization",
  "repository": {
    "type": "git",
    "url": "https://github.com/cncolder/vscode-source-map-visualization"
  },
  "bugs": {
    "url": "https://github.com/cncolder/vscode-source-map-visualization/issues"
  },
  "keywords": [
    "Source Map",
    "Visualization",
    "sourcemap",
    "source-map",
    "sourcemaps",
    "map",
    ".map",
    "jsmap",
    ".js.map",
    "cssmap",
    ".css.map"
  ],
  "categories": [
    "Visualization"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onCommand:sourceMapVisualization.show"
  ],
  "contributes": {
    "commands": [
      {
        "command": "sourceMapVisualization.show",
        "title": "Source Map Visualization"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "sourceMapVisualization.show",
          "group": "z_commands"
        }
      ]
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "vscode:pack": "vsce package --no-dependencies",
    "vscode:prepublish": "pnpm build",
    "vscode:publish": "vsce publish --no-dependencies",
    "release": "bumpp -x 'pnpm vscode:publish'"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@types/node": "^18.11.14",
    "@types/vscode": "1.69.0",
    "@vscode/vsce": "^2.15.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vitest": "^0.25.8"
  }
}
